package stringsWithSameLetters;

import java.util.ArrayList;
import java.util.Scanner;

public class StringsWithSameLetters {

	public static void main(String[] args) {
		ArrayList<String> arrList = new ArrayList<>();
		Scanner scan = new Scanner(System.in);
		System.out.println("Enter");
		while (scan.hasNextLine()) {
			String a = scan.nextLine();
			String b = scan.nextLine();
			if (a.equals("END") && b.equals("END")) {
				break;
			}
			arrList.add(a);
			arrList.add(b);
		}
		scan.close();

		for (int i = 0; i < arrList.size() / 2; i++) {
			System.out.print("Case " + (i + 1) + ": ");
			if (findMatch(arrList.get(2 * i), arrList.get(2 * i + 1))) {
				System.out.println("same");
			} else {
				System.out.println("different");
			}
		}

	}

	public static boolean findMatch(String a, String b) {
		if (a.length() != b.length()) {
			return false;
		} else {
			boolean result = false;
			for (int i = 0; i < a.length(); i++) {
				for (int j = i; j < a.length(); j++) {
					boolean matchChar = false;
					if (a.charAt(i) == b.charAt(j)) {
						matchChar = true;
						swap(b, i, j);

					}
				}
			}
			return result;
		}

	}

	/**
	 * 
	 * @param a
	 * @param indexA
	 * @param indexB
	 */
	public static void swap(String a, int indexA, int indexB) {
		char temp = a.charAt(indexA);

		return result;
	}
}
