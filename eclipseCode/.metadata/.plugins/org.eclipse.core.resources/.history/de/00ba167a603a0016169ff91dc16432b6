package stringsWithSameLetters;

import java.util.ArrayList;
import java.util.Scanner;

public class Junk {

	public static void main(String[] args) {
		int count = 0;
		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < 5; j++) {
				
				System.out.println(count);
				count++;
				if (j == 1)
					break;
			}
		}

	}

	public static void swap(String a, int indexA, int indexB) {
		if (indexA == indexB)
			return;

		String first = a.substring(0, indexA);
		String second = a.substring(indexA + 1, indexB);
		String third = a.substring(indexB + 1, a.length());

		a = first + a.charAt(indexB) + second + a.charAt(indexA) + third;
	}

}
